@model List<ProductRevenueViewModel>

@{
    ViewData["Title"] = "ViewRevenue";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>ViewRevenue</h1>
<table>
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Revenue</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductName</td>
                <td>@product.Revenue</td>
            </tr>
        }

    </tbody>
</table>