@model List<ApplicationUser>

@{
    ViewData["Title"] = "ViewStaff";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Staff List</h1>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var staff in Model)
        {
            <tr>
                <td>@staff.Id</td>
                <td>@staff.UserName</td>
                <td>@staff.Email</td>
                <td>@staff.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>

